            skos:prefLabel "Place"@en .
            
            cxt:Highway a skos:Concept ;
                skos:broader cxt:Place ;
                rdfs:seeAlso <https://de.wikipedia.org/wiki/Autobahn>,
                    <https://en.wikipedia.org/wiki/Controlled-access_highway> ;
                skos:prefLabel "Autobahn"@de,
                    "Highway"@en .

        cxt:PhysicalObject a skos:Concept ;
            skos:broader cxt:Thing ;
            skos:prefLabel "Physical Object"@en .
        
        cxt:Part a skos:Concept ;
            skos:broader cxt:PhysicalObject ;
            skos:prefLabel "Part"@en .
            
            cxt:NaturalGasVehicle a skos:Concept ;
                skos:broader cxt:Part ;
                skos:altLabel "NGV"@en,
                    "natural gas vehicle"@en ;
                skos:prefLabel "Erdgasfahrzeug"@de,
                    "Natural Gas Vehicle"@en .
            
            cxt:ConstructionPart a skos:Concept ;
                skos:broader cxt:ConceptionalObject ;
